<?php

function tri_insertion($array) {
  $array_length = count($array);
  for ($i = 0; $i < $array_length; $i++) {
    $x = $array[$i];
    for ($j = $i; $j > 0 && $array[$j - 1] > $x; $j--)
      $array[$j] = $array[$j - 1];
    $array[$j] = $x;
  }
  return ($array);
}


$arr = [4,9,32,5,8,9,4,3,6,7,1,2,3,6,9,85,78];
print_r($arr);
$arr = tri_insertion($arr);
print_r($arr);
