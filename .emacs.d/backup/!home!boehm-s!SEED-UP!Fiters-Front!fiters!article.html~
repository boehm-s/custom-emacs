    <script type="text/javascript">
     $(document).ready(function(){
       var s= $(".slick-phone"),
           p= $(".phone-f-b"),
           h= p.height(),
           r= 308/540
       n= h*r;

       var c = $(".iblock-coach-img").parent().height();
       if($(window).width()>991) {
         $(".iblock-coach-text").height(c);
       }
       //console.log(c)

       if($(window).width()<991) {
         console.log(n)
         // s.height(n);
       }

       $("#progress0").addClass("on");
       setTimeout(function(){
         $("#bar1").addClass("active");
       }, 400);
       var slider = $('#fiters-slider').unslider({ autoplay: true, delay: 3000, arrows: false, nav: false });

       //  When the slider has been set up, fire the event off.
       slider.on('unslider.ready', function() {
         alert('Slider is set up!');
       });

       slider.on('unslider.change', function(event, index, slide) {
         var i = index;
         $("#progress"+i).addClass("on");
         changeClassPhone();
       });

       function changeClassPhone() {
         if($("#slide-f-1").hasClass("unslider-active")) {
           $("#iphone-pos").removeClass();
           $("#iphone-pos").addClass("left1");
           $(".nbr-progress").removeClass("on");
           $("#progress0").addClass("on");
           setTimeout(function(){
             $("#bar1").addClass("active");
           }, 400);

         }
         if($("#slide-f-2").hasClass("unslider-active")) {
           $("#iphone-pos").removeClass();
           $("#iphone-pos").addClass("left2");
           $("#bar2").addClass("active");

         }
         if($("#slide-f-3").hasClass("unslider-active")) {
           $("#iphone-pos").removeClass();
           $("#iphone-pos").addClass("left3");
           setTimeout(function(){
             $(".progress-bar").removeClass("active");
           }, 2800);

         }
       };
     });
    </script>

