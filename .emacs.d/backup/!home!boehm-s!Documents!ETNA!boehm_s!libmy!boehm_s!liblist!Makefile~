AR	= ar r
LIB	= ranlib
NAME	= libmy.a
SRC	= 	list.c	\

OBJ	= $(SRC:%.c=%.o)

$(NAME):	$(OBJ)
		$(AR) $(NAME) $(OBJ)
		ranlib $(NAME)
all:		$(NAME)
clean:
		$(RM) $(OBJ)
fclean:		clean
		$(RM) $(NAME)
re: 		fclean all
