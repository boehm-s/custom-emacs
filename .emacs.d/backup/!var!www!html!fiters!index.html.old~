    <!-- JS Fiters-->
    <script src="assets/scripts/libs/jquery.min.js" type="text/javascript"></script>
    <script src="assets/scripts/libs/classie.js" type="text/javascript"></script>

    <script src="assets/scripts/libs/video.js/dist/video.min.js" type="text/javascript"></script>
    <script src="assets/scripts/libs/videojs-youtube/dist/Youtube.min.js" type="text/javascript"></script>

    <script src="assets/scripts/fct.js" type="text/javascript"></script>
    <script src="assets/scripts/init.js" type="text/javascript"></script>

    <script>
     var $root       = $('body'),
         $wh         =  $(window).width(),
         $ratio      =  315/560,
         $newHeight  =  $ratio * $wh;

     if ($("html").hasClass("desktop")) {
       videojs("example_video_1").ready(function(){
         var myPlayer = this;    // Store the video object
         var aspectRatio = 315/560; // Make up an aspect ratio
         var $vh = $wh*0.9

         myPlayer.muted(true);

         function resizeVideoJS(){
           $("#example_video_1").width($wh).height( $wh  * aspectRatio );
           $(".video-bg").width($vh).height( $vh  * aspectRatio - 50  );
           document.getElementById("example_video_1").style.visibility = "visible";
         }
         resizeVideoJS(); // Initialize the function
         window.onresize = resizeVideoJS; // Call the function on resize
       });
     }

     $(document).ready(function(){
       $scrollPosition = $(window).scrollTop(),
       $sliderPosition = $("#slider-phone").offset().top;
       $CoachPosition = $("#coach-volet").offset().top;
       $windowH        = $(window).height();



       $(".slideContent").height($windowH-75);


       var isTop = window.scrollY < document.getElementById('slider-phone').offsetTop;
       var isBot = !isTop;
       var sliderPhone = document.getElementById('slider-phone');
       var phonebox = document.getElementsByClassName('phonebox')[0];
       var slideContent = document.getElementsByClassName('slideContent');
       var step1 = 0,
	   step2 = 0,
	   step3 = 0;
       var nbr1 = document.getElementsByClassName('nbr-progress')[0],
	   nbr2 = document.getElementsByClassName('nbr-progress')[1],
           nbr3 = document.getElementsByClassName('nbr-progress')[2];
       var progress1 = document.getElementsByClassName('progress-bar-info')[0],
           progress2 = document.getElementsByClassName('progress-bar-info')[1];

       $(document).scroll(function () {

	 if (!window.matchMedia("screen and (max-width: 992px)").matches) {


           if($(window).scrollTop() >= $sliderPosition-75 && $(window).scrollTop() < $("#coach-volet").offset().top-$windowH) { // user is on the phone view
	     isTop = false;
	     isBot = false;
             $(".phonebox").addClass("fixe-mobile");
             $(".progressBar").addClass("fixed");
	     phonebox.style.marginTop = "0px";

	     // progress bar
	     step1 = slideContent[0].getBoundingClientRect().top / slideContent[0].offsetHeight * 100;
	     step2 = slideContent[1].getBoundingClientRect().top / slideContent[1].offsetHeight * 100;
	     step3 = slideContent[2].getBoundingClientRect().top / slideContent[2].offsetHeight * 100;

	     nbr1.className = (step1 < 50) ? "nbr-progress on" : "nbr-progress";
	     nbr2.className = (step2 < 50) ? "nbr-progress on" : "nbr-progress";
	     nbr3.className = (step3 < 50) ? "nbr-progress on" : "nbr-progress";

	     progress1.style.height = Math.abs(step1) > 100 ? 100 + "%" : Math.abs(step1) + "%";
	     if (step2 > 0)
	       progress2.style.height = "0%";
	     else
	       progress2.style.height = Math.abs(step2) > 100 ? 100 + "%" : Math.abs(step2) + "%";

           } else { // user leave the phone view
	     if (window.scrollY < document.getElementById('slider-phone').offsetTop) { // user is on the top of the page
	       isTop = true;
	       isBot = false;
	     } else { // user is on the bottom of the page
	       if (!isBot)
		 phonebox.style.marginTop = (document.getElementById('slider-phone').offsetHeight - phonebox.offsetHeight) + "px";
	       isTop = false;
	       isBot = true;
	     }

             $(".phonebox").removeClass("fixe-mobile");
             $(".progressBar").removeClass("fixed");
           }
	 }
       });
     });

    </script>

